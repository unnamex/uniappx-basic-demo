<script lang="uts">
	import { ProcessListItem } from '@/types/process'
	import { getProcessCount } from '@/services/processService'
	import { initDatabase } from '@/services/database'
	
	/**
	 * 最近查看的工艺项类型
	 */
	type RecentProcess = {
		id: string
		name: string
		code: string
	}
	
	export default {
		data() {
			return {
				// 工艺总数
				processCount: 0,
				
				// 最近导入时间
				lastImportTime: '-',
				
				// 最近查看的工艺
				recentProcesses: [] as RecentProcess[],
				html: "<table style=\"border-collapse: collapse; width: 100%; height: 51px;\" border=\"1\"><colgroup><col style=\"width: 50%;\"><col style=\"width: 50%;\"></colgroup>\n<tbody>\n<tr style=\"height: 27px;\">\n<td><span style=\"background-color: #f1c40f;\">爱的色放打算fdasfdas</span></td>\n<td><span style=\"background-color: #f1c40f;\">&nbsp;<strong>范德萨发生大</strong></span></td>\n</tr>\n<tr style=\"height: 24px;\">\n<td><span style=\"background-color: #f1c40f;\">发送范德萨<img src=\"data:image/png;base64,\" alt=\"\" width=\"966\" height=\"486\"></span></td>\n<td><span style=\"color: #e03e2d; font-size: 48px;\">空间划分了肯定撒开</span></td>\n</tr>\n</tbody>\n</table>\n<p><img src=\"data:image/png;base64,\"></p>"
		},
		
		onShow() {
			this.loadData()
		},
		
		methods: {
			/**
			 * 加载数据
			 */
			loadData() {
				initDatabase()
					.then(() => {
						return getProcessCount()
					})
					.then((count: number) => {
						this.processCount = count
					})
					.catch((e: Error) => {
						console.error('加载数据失败：', e.message)
					})
				
				// 加载最近查看记录（从本地存储）
				this.loadRecentProcesses()
			},
			
			/**
			 * 加载最近查看记录
			 */
			loadRecentProcesses() {
				try {
					const recentStr = uni.getStorageSync('recent_processes') as string
					if (recentStr != '') {
						this.recentProcesses = JSON.parse(recentStr) as RecentProcess[]
					}
				} catch (e: any) {
					console.error('加载最近记录失败：', JSON.stringify(e))
				}
			},
			
			/**
			 * 跳转到工艺列表
			 */
			goToProcessList() {
				uni.switchTab({
					url: '/pages/process/list'
				})
			},
			
			/**
			 * 跳转到数据导入
			 */
			goToImport() {
				uni.switchTab({
					url: '/pages/import/import'
				})
			},
			
			/**
			 * 跳转到设置页面
			 */
			goToSettings() {
				uni.navigateTo({
					url: '/pages/settings/settings'
				})
			},
			
			/**
			 * 跳转到工艺详情
			 */
			goToDetail(processId: string) {
				uni.navigateTo({
					url: '/pages/process/detail?id=' + processId
				})
			}
		}
	}
</script>

